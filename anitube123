<script>
        $(function() {
            var scroll_start = 10;
            var top = 0;
            $('body').append('<a href="#" id="gotop"></a>');
            var s = $('#gotop');
            var margin_top = parseInt(s.css('top'));

            function gotop_scroll() {
                top = $(window).scrollTop();
                if (top < scroll_start) s.fadeOut(100);
                else s.css('opacity', 0.5).fadeIn(100)
            }
            $(window).scroll(gotop_scroll);
            s.live({
                mouseenter: function() {
                    if (top > scroll_start) $(this).fadeTo(100, 1.0)
                },
                mouseleave: function() {
                    if (top > scroll_start) $(this).fadeTo(100, 0.5)
                },
                click: function() {
                    $('html,body').animate({
                        scrollTop: 0
                    }, 'slow');
                    return false
                }
            })
        })
    </script>
