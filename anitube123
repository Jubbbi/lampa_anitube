<div id="dle-content"><script type="text/javascript">
var loading = 1;

function load_episode()
{
	loading = 1;
	let id = 4295;
	let episode = localStorage.getItem('episode' + id);
	if(episode)
	{
		episode = episode.split(':');
		if(episode[0])
		{
			let tabs = episode[0].split('|');
			let playlists = jQuery('.playlists-lists .playlists-items');
			for(let i = 0, n = playlists.length; i < n; i++)
			{
				jQuery(playlists).eq(i).find('li:visible').eq(tabs[i]).click();
			}
		}
		jQuery('.playlists-videos li:visible').eq(episode[1]).click();	
	}
	loading = 0;
}

jQuery(window).on('load', function(){load_episode();});
</script>
